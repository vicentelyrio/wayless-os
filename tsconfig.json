{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "ags/gtk4",
    "lib": [
      "ES2023"
    ],
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "paths": {
      "@*": [
        "./src/*"
      ]
    },
    "strict": true,
    "target": "ES2020"
  }
}